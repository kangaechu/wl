<data key='user.appserver.tomcat.installdir' value='<%= node["wl"]["tomcat"]["installdir"] %>'/>
<data key='user.writable.data.group2' value=''/>
<data key='cic.selector.nl' value='en'/>
</profile>
<install modify='false'>
<offering id='com.ibm.imp' version='6.0.0.20130614_0652' profile='Worklight' features='main.feature' installFixes='none'/>
</install>
<preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='/opt/IBM/IBMIMShared'/>
<preference name='com.ibm.cic.common.core.preferences.connectTimeout' value='30'/>
