<data key='user.database.mysql.host' value='<%= node["wl"]["mysql"]["host"] %>'/>
<data key='user.database.mysql.port' value='<%= node["wl"]["mysql"]["port"] %>'/>
<data key='user.database.mysql.username' value='<%= node["wl"]["mysql"]["username"] %>'/>
<data key='user.database.mysql.password2' value='<%= node["wl"]["mysql"]["password2"] %>'/>
